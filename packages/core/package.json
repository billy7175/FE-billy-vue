{
  "name": "@billy7175/my-ui-core",
  "version": "0.0.1",
  "description": "Vue 3 컴포넌트 라이브러리 - 실무형 UI 키트",
  "private": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "style": "dist/styles.css",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./plugin": {
      "import": "./dist/plugin.js",
      "require": "./dist/plugin.cjs",
      "types": "./dist/plugin.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "typecheck": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "sass": "^1.69.0",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.9.0",
    "vue": "^3.4.0",
    "vue-tsc": "^1.8.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "component-library",
    "ui-kit"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/billy7175/FE-billy-vue.git"
  }
}

